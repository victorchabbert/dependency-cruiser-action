{
  "name": "dependency-cruiser-action",
  "version": "0.0.0",
  "private": true,
  "description": "Report dependency-cruiser results",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.{ts,js}",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build lib/main.js --source-map -e dependency-cruiser",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "depcruise": "depcruise"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorchabbert/dependency-cruiser-action.git"
  },
  "keywords": [
    "actions",
    "dependency",
    "dependency-cruiser",
    "depcruise"
  ],
  "author": "Victor Chabbert",
  "license": "MIT",
  "prettier": "@github/prettier-config",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "dependency-cruiser": "^9.25.0",
    "markdown-table": "^3.0.0"
  },
  "devDependencies": {
    "@github/prettier-config": "0.0.4",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/parser": "^4.21.0",
    "@vercel/ncc": "^0.28.1",
    "eslint": "^7.23.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.4",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
